{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "title": "preview",
  "description": "An Adapt course preview instance",
  "type": "object",
  "properties": {
    "courseId": {
      "description": "The course _id",
      "type": "string"
    },
    "expiresAt": {
      "description": "When the preview should be purged from the server",
      "type": "string"
      "format": "date-time"
    }
  },
  "required": ["courseId", "expiresAt"]
}
